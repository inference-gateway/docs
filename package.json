{
  "name": "inference-gateway-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:md": "markdownlint \"**/*.md\" \"**/*.mdx\" --ignore \"**/node_modules/**\"",
    "lint:md:fix": "markdownlint \"**/*.md\" \"**/*.mdx\" --ignore \"**/node_modules/**\" --fix",
    "generate-sitemap": "tsx scripts/generate-sitemap.ts",
    "generate-search-index": "tsx scripts/generate-search-index.ts",
    "prebuild": "npm run generate-search-index && npm run generate-sitemap",
    "serve": "next build && npx serve out",
    "clean": "rm -rf out && rm -rf .next"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.5.2",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@types/mdx": "^2.0.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.488.0",
    "mermaid": "^11.10.0",
    "next": "^15.5.2",
    "next-themes": "^0.4.6",
    "prismjs": "^1.30.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/glob": "^8.1.0",
    "@types/node": "^20",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.16",
    "eslint": "^9",
    "eslint-config-next": "^15.5.2",
    "eslint-config-prettier": "^10.1.2",
    "glob": "^11.0.1",
    "husky": "^9.1.7",
    "markdownlint": "^0.38.0",
    "markdownlint-cli": "^0.45.0",
    "postcss": "^8.4.31",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "tsx": "^4.19.3",
    "typescript": "^5"
  }
}
